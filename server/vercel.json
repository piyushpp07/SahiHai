{
    "version": 2,
    "buildCommand": "npm run build",
    "functions": {
        "api/index.js": {
            "runtime": "@vercel/node@3.0.0",
            "memory": 1024,
            "maxDuration": 60
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}