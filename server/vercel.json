{
    "version": 2,
    "buildCommand": "npm run build",
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node@3.0.0",
            "config": {
                "nodeVersion": "18.x"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/index.js"
        }
    ]
}